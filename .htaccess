RewriteEngine on
RewriteOptions inherit
RewriteCond %{HTTP_HOST} ^concejomunicipalescuque\.com$
RewriteCond %{HTTP_HOST} ^www\.concejomunicipalescuque\.com$
RewriteRule ^$ "https\:\/\/concejomunicipalescuque\.com" [R=301,L]


<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_URI} !^/public/
    RewriteRule ^(.*)$ /public/$1 [L]
</IfModule>